<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="insertCategory">
	
	<insert id="insertCategoryS" parameterType="com.main.admin.product.VO.CategorySVO" >
		<![CDATA[insert into small_class values(#{code}, #{big_class_code}, #{name})]]>
	</insert>
	<select id="selectBigCategory" parameterType="com.main.admin.product.VO.CategoryVO" resultType="com.main.admin.product.VO.CategoryVO">
		<![CDATA[select * from big_class]]>
	</select>
</mapper>