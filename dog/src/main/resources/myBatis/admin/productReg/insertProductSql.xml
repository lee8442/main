<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ProductReg"  >
	
	<!-- 상품 등록 -->
	<insert id="regProduct"  parameterType="ProductVO">
	insert into product (code,name,price,stock,explain,simple_explain)values(#{code},#{name},#{price},#{stock},#{explain},#{simple_explain})
	</insert>
	
</mapper>
