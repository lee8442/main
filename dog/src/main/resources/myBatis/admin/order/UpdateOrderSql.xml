<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="updateOrder">

	<update id="OrderDeliveryStart" parameterType="com.main.admin.order.VO.OrderVO" >
		<![CDATA[update product_order set delivery_code=0 where code = #{code}]]>
	</update>
	
	<update id="OrderDeliveryEnd" parameterType="com.main.admin.order.VO.OrderVO" >
		<![CDATA[update product_order set delivery_code=1 where code = #{code}]]>
	</update>
</mapper>