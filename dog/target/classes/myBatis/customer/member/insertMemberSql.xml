<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myBatis.customer.member">
	<!-- <resultMap id="map"
		type="com.main.customer.member.VO.MemberVO">
		<result property="id" column="id" jdbcType="VARCHAR" />
	</resultMap> -->
	
	<insert id="insertMember"
		parameterType="com.main.customer.member.VO.MemberVO">
		insert into member values(#{id}, #{name}, #{password},
		#{registration},
		#{address}, #{email}, sysdate, #{phone}, #{point})
	</insert>

	<select id="idCheck" parameterType="String" resultType="int">
		<![CDATA[select count(id) from member where id = #{id}]]>
	</select>
</mapper>